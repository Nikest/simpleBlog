{
    "extends": "extends": ["eslint:recommended", "plugin:react/recommended"],
    "plugins": [
        "react"
    ],
    "rules": {
        "interface-name": [true, "always-prefix"],
        "max-line-length": [true, 120],
        "variable-name": [true, "allow-pascal-case"],
        "react/no-set-state": [true],
        "react/no-did-mount-set-state": [true, "disallow-in-func"],
        "react/no-did-update-set-state": [true, "disallow-in-func"]
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
